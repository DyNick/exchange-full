								span.value-box-percent.value-box-percent--hour 233

								span.value-box-many.value-box-many--hour

						div.card-container__item__time__day.card-container__item__time--row
							span.card-container__item__time__title Day change
							div.card-container__item__time__value-box
								span.value-box-percent.value-box-percent--day 233

								span.value-box-many.value-box-many--day

						div.card-container__item__time__week.card-container__item__time--row
